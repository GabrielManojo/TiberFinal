<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation"
    app:startDestination="@+id/nav_home">

    <fragment
        android:id="@+id/nav_home"
        android:name="com.example.Tiber.ui.home.HomeFragment"
        android:label="@string/menu_home"
        tools:layout="@layout/fragment_home" />

    <fragment
        android:id="@+id/nav_profile"
        android:name="com.example.Tiber.ui.profile.nav_profile"
        android:label="fragment_profile"
        tools:layout="@layout/fragment_profile" />
    <fragment
        android:id="@+id/nav_driver_activity"
        android:name="com.example.Tiber.ui.driverActivity.DriverHistoryFragment"
        android:label="fragment_driver_activity"
        tools:layout="@layout/fragment_driver_history" />

    <fragment
        android:id="@+id/nav_help"
        android:name="com.example.Tiber.ui.Support.nav_help"
        android:label="fragment_help"
        tools:layout="@layout/fragment_help" />

